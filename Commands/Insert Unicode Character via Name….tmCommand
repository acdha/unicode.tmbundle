	&lt;script&gt;
		var myCommand = null;
		outStr = "";
		function startSearch(term) {
			var w = (document.getElementById("where").checked) ? 'full':'word';
			if (term.length &gt; 0) {
				TextMate.isBusy = true;
				outStr = "";
				var cmd = "/usr/bin/python '$TM_BUNDLE_SUPPORT/bin/SearchUNInames.py' '"+w+"' '"+term+"'";
